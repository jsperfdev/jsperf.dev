{
  "name": "jsperf.dev",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,ts,md}\"",
    "prepare": "husky install",
    "test": "turbo run test"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "turbo": "^1.3.4"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.6.0",
  "contributors": [
    "Ethan Arrowood <ethan@arrowood.dev>"
  ],
  "lint-staged": {
    "**/*.{ts,tsx,md}": "prettier --write"
  }
}
