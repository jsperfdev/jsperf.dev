    "@types/node": "^16.11.45",
    "@types/sinon": "^10.0.13",
    "@types/tap": "^15.0.6",
    "sinon": "^14.0.0",
    "tap": "^16.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  }
